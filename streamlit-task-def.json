{
  "family": "housing-streamlit",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "housing-streamlit",
      "image": "261899902410.dkr.ecr.eu-west-2.amazonaws.com/housing-streamlit:latest",
      "portMappings": [
        {
          "containerPort": 8501,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "executionRoleArn": "arn:aws:iam::261899902410:role/ecsTaskExecutionRole"
}
